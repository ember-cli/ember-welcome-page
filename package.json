{
  "name": "ember-welcome-page",
  "version": "4.0.0",
  "description": "Welcome page for Ember CLI applications",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "David Baker <acorncom@gmail.com>",
  "repository": "https://github.com/ember-cli/ember-welcome-page",
  "scripts": {
    "build": "cd test-app && ember build --environment=production",
    "changelog": "lerna-changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint test-app",
    "lint:js": "eslint .",
    "start": "cd test-app && ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "cd test-app && ember test",
    "test:ember-compatibility": "cd test-app && ember try:each"
  },
  "dependencies": {
    "@embroider/util": "^0.38.0-alpha.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "ember-auto-import": "^1.10.1",
    "ember-template-lint": "^2.21.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lerna-changelog": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./components/welcome-page.js": "./app/components/welcome-page.js"
    },
    "public-assets": {
      "./images/construction.png": "/ember-welcome-page/images/construction.png"
    }
  }
}
